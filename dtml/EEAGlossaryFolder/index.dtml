<dtml-in get_object_list>
    <dtml-if is_published>
        <dtml-if synonyms>
            <img src="misc_/EEAGlossary/element.gif" border=0>&nbsp;<dtml-var name>&nbsp;<img src="misc_/EEAGlossary/new.gif" border=0>
            <dtml-in synonyms size=1>
                <dtml-if sequence-start>
                    (<small>See </small>
                </dtml-if><dtml-comment>
                <dtml-if "_.len(_['sequence-item'])>1">
                    <dtml-in "GlossaryCatalog(id=_['sequence-item'])">
                        <dtml-with "getObject()">
                            <a href="&dtml-absolute_url;"><dtml-var name_or_id></a>)
                        </dtml-with>
                    </dtml-in>
                </dtml-if></dtml-comment>
            </dtml-in>
        <dtml-else>
              <a href="<dtml-var absolute_url>"><img src="misc_/EEAGlossary/element.gif" border=0>&nbsp;<dtml-var name>&nbsp;<img src="misc_/EEAGlossary/new.gif" border=0></a>
        </dtml-if>
    <br>
    </dtml-if>
</dtml-in>

